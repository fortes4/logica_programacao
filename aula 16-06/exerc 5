/*5) Imprima uma tabela de conversão de polegadas para centímetros, de 1 a 20. Considere que Polegada = Centímetro * 2,54.*/

#include <stdio.h>
#include <conio.h>

main(){
    float cm, polegada = 0;
    
    for(cm=1 ; cm<=20 ; cm++) {
    	polegada = cm * 2.54;
   		 printf("%.2f - %.2f\n", cm,polegada);
  }

    return 0;
}
